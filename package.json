  "version": "1.0.0",
  "main": "src/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "test": "jest",
    "commit": "git-cz"
  },
  "dependencies": {
    "cors": "^2.8.5",
